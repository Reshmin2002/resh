#include<iostream>
using namespace std;
int main()
{
    int i,j,n;
    cin>>n;
    for(i=0;i<n;i++)
    {
        for(j=1;j<2*i+1;j++)
        {
            if(j%2!=0)
            {
                cout<<j;
            }
        }
        while(j>0)
        {
            if(j%2!=0)
            {
                cout<<j--;
            }
            j--;
        }
        cout<<endl;
    }
}
